{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates" : {
        "github" : "https://github.com/MapTalks/maptalks.js/blob/master/src/",
        "categories" : "map, layer, geometry, basic types ,maptool, ui, control",
        "default" : {
            "useLongnameInNav" : true
        }
    },

    "opts": {
        "recurse" : true,
        "template": "../maptalks-jsdoc/",
        "encoding": "utf8",
        "destination": "./doc/api/",
        "access"  : "public, undefined"
    }
}
